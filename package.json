{
    "name": "beat-saver-client",
    "version": "0.7.0-alpha",
    "main": "dist/app/index.js",
    "description": "A BeatSaver desktop client",
    "author": {
        "name": "Nick K.",
        "email": "n.koellhofer@googlemail.com"
    },
    "repository": {
        "url": "https://github.com/dotMortis/beat-saver-client"
    },
    "cli": {
        "packageManager": "yarn"
    },
    "scripts": {
        "prestart": "yarn clean",
        "start:dev": "concurrently -k \"yarn ng:serve:dev\" \"yarn electron:serve\"",
        "ng:serve:dev": "ng serve -c development",
        "electron:tsc": "tsc -p tsconfig.electron.json",
        "electron:serve": "yarn clean && yarn copy && wait-on tcp:4200 && yarn electron:tsc && npx electron . --serve --debug",
        "electron:serve:nf": "yarn clean && yarn copy && yarn electron:tsc && npx electron . --serve --debug",
        "prebuild": "yarn clean && yarn copy",
        "build": "yarn electron:tsc && ng build --configuration production",
        "build:prod": "yarn build -- -c production",
        "test": "ng test",
        "lint": "ng lint --fix",
        "e2e": "ng e2e",
        "release:pub": "yarn build:prod && electron-builder build --win -p onTagOrDraft",
        "release:dev": "yarn build:prod && electron-builder build --win -p never",
        "clean": "rimraf dist && rimraf release",
        "copy": "copyfiles src/assets/splash.png app/html/**/*.html dist/",
        "analyze": "webpack-bundle-analyzer dist/ui/stats.json",
        "rebuild:sqlite": "./node_modules/.bin/electron-rebuild -f -w better-sqlite3"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~12.2.3",
        "@angular/cdk": "12.2.3",
        "@angular/common": "~12.2.3",
        "@angular/compiler": "~12.2.3",
        "@angular/core": "~12.2.3",
        "@angular/forms": "~12.2.3",
        "@angular/localize": "~12.2.3",
        "@angular/platform-browser": "~12.2.3",
        "@angular/platform-browser-dynamic": "~12.2.3",
        "@angular/router": "~12.2.3",
        "@fortawesome/angular-fontawesome": "0.9",
        "@fortawesome/fontawesome-svg-core": "^1.2.35",
        "@fortawesome/free-brands-svg-icons": "^5.15.3",
        "@fortawesome/free-regular-svg-icons": "^5.15.3",
        "@fortawesome/free-solid-svg-icons": "^5.15.3",
        "@ng-bootstrap/ng-bootstrap": "10",
        "better-sqlite3": "^7.4.3",
        "bootstrap": "^4.5.0",
        "electron-updater": "^4.3.9",
        "jszip": "^3.7.1",
        "ngx-joyride": "^2.4.0",
        "ngx-markdown": "12",
        "primeicons": "^4.1.0",
        "primeng": "11.4.4",
        "rxjs": "6",
        "tslib": "^2.3.0",
        "uuid": "^8.3.2",
        "winston": "^3.3.3",
        "winston-daily-rotate-file": "^4.5.5",
        "zone.js": "~0.11.4"
    },
    "devDependencies": {
        "@angular-builders/custom-webpack": "12",
        "@angular-devkit/build-angular": "~12.2.3",
        "@angular-eslint/builder": "^12.0.0",
        "@angular-eslint/eslint-plugin": "^12.0.0",
        "@angular-eslint/eslint-plugin-template": "^12.0.0",
        "@angular-eslint/schematics": "~12.3.1",
        "@angular-eslint/template-parser": "^12.0.0",
        "@angular/cli": "~12.2.3",
        "@angular/compiler-cli": "12",
        "@types/better-sqlite3": "^7.4.0",
        "@types/jasmine": "~3.8.0",
        "@types/jszip": "^3.4.1",
        "@types/node": "^16.4.13",
        "@types/uuid": "^8.3.1",
        "@typescript-eslint/eslint-plugin": "4.28.2",
        "@typescript-eslint/parser": "4.28.2",
        "concurrently": "^6.2.1",
        "copyfiles": "^2.4.1",
        "electron": "^13.1.8",
        "electron-builder": "^22.11.7",
        "electron-rebuild": "^3.2.3",
        "eslint": "^7.26.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^3.4.0",
        "jasmine-core": "~3.8.0",
        "karma": "~6.3.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage": "~2.0.3",
        "karma-jasmine": "~4.0.0",
        "karma-jasmine-html-reporter": "~1.7.0",
        "prettier": "^2.3.2",
        "rimraf": "^3.0.2",
        "typescript": "4.2.4",
        "wait-on": "^6.0.0",
        "webpack-bundle-analyzer": "^4.4.2"
    },
    "engines": {
        "node": ">=12.0.0"
    }
}
